{
  "version": "1.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "NODE_ENV=production tsdx build --format cjs",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "NODE_ENV=production tsdx build  --format cjs",
    "size": "NODE_ENV=production size-limit",
    "analyze": "NODE_ENV=production size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "r3f-perf",
  "author": "onirenaud",
  "size-limit": [
    {
      "path": "dist/r3f-perf.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/r3f-perf.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-three-fiber": "*",
    "three": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-three-fiber": "^5.3.18",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.9.2",
    "three": "^0.126.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@types/three": "^0.126.2",
    "lerp": "^1.0.3",
    "zustand": "^3.3.3"
  }
}

{
  "name": "r3f-perf",
  "version": "7.0.0",
  "description": "Easily monitor your ThreeJS performances.",
  "keywords": [
    "react",
    "three",
    "benchmark",
    "performance",
    "monitor",
    "analysis",
    "profiling",
    "r3f"
  ],
  "author": "Renaud ROHLINGER (https://github.com/RenaudRohlinger)",
  "homepage": "https://github.com/utsuboco/r3f-perf",
  "repository": "https://github.com/utsuboco/r3f-perf",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./headless": {
      "types": "./dist/headless.d.ts",
      "require": "./dist/headless.js",
      "import": "./dist/headless.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "serve": "vite serve"
  },
  "devDependencies": {
    "@react-three/fiber": "^8.10.1",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/three": "^0.148.1",
    "@vitejs/plugin-react": "^3.0.1",
    "leva": "^0.9.34",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.149.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.0.3",
    "@react-three/drei": "^9.56.9",
    "@stitches/react": "^1.2.8",
    "three-stdlib": "^2.20.5",
    "zustand": "~4.1.5"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0",
    "react": ">=18.0",
    "react-dom": ">=18.0",
    "three": ">=0.133"
  },
  "peerDependenciesMeta": {
    "@react-three/fiber": {
      "optional": true
    },
    "dom": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
